<div class="container-fluid">
  <div class="row-fluid">
    <div class="span12">

      <div class="box bordered">
        <div class="box-header">
          <i class="icon-bullhorn"></i> Interface elements
        </div>

        <div class="row-fluid">
          <div class="span6">
            <div class="padded">
              <div class="section-title">Icon labels</div>
              <div class="vpadded">
                <%= partial "/partials/components/elements/big_labels" %>
              </div>
              <div class="vpadded">
                <%= partial "/partials/components/elements/big_labels_round" %>
              </div>
            </div>
            <div class="padded">
              <div class="section-title">Datepickers</div>
              <div class="vpadded">
                <%= partial "/partials/components/elements/datepicker" %>
              </div>
            </div>
          </div>

          <div class="span6">
            <div class="padded">
              <div class="section-title">Progress bars</div>
              <%= partial "/partials/components/elements/progress" %>
            </div>
          </div>
        </div>

        <div class="row-fluid">
          <div class="span6">
            <div class="padded">
              <div class="section-title">Labels and badges</div>
              <%= partial "/partials/components/elements/labels_blue" %>
            </div>
          </div>
          <div class="span6">
            <div class="padded">
              <div class="section-title">Labels and badges: EXTRA COLORS</div>
              <%= partial "/partials/components/elements/labels" %>
            </div>
          </div>
        </div>

        <div class="row-fluid">
          <div class="span12">
            <div class="padded">
              <div class="section-title">Circle stats</div>
              <%= partial "/partials/components/elements/circle_stats" %>
            </div>
          </div>
        </div>

        <div class="row-fluid">
          <div class="span12">
            <div class="padded">
              <div class="section-title">Tags</div>
              <%= partial "/partials/components/elements/tags" %>
            </div>
          </div>
        </div>

        <div class="row-fluid">
          <div class="span5">
            <div class="padded">
              <div class="section-title">Horizontal sliders</div>
              <%= partial "/partials/components/elements/sliders_horizontal" %>
            </div>
          </div>
          <div class="span7">
            <div class="padded">
              <div class="section-title">Vertical sliders</div>
              <%= partial "/partials/components/elements/sliders_vertical" %>
            </div>
          </div>
        </div>

        <div class="row-fluid">
          <div class="span6">
            <div class="padded">
              <div class="section-title">Alerts</div>
              <%= partial "/partials/components/elements/alerts" %>
            </div>
          </div>
          <div class="span6">
            <div class="padded">
              <div class="section-title">Block alerts</div>
              <%= partial "/partials/components/elements/alerts_block" %>
            </div>
          </div>
        </div>

        <div class="row-fluid">
          <div class="span8">
            <div class="padded">
              <div class="section-title">Quick links</div>
              <%= partial "/partials/components/elements/quick_links" %>
            </div>
          </div>
          <div class="span4">
            <div class="padded">
              <div class="section-title">Growl</div>
              <%= partial "/partials/components/elements/notifications" %>
            </div>
          </div>
        </div>

        <div class="row-fluid">
          <div class="span7">
            <div class="padded">
              <div class="section-title">Accordions</div>
              <%= partial "/partials/components/elements/accordion" %>
            </div>
          </div>
          <div class="span5">
            <div class="padded">
              <div class="section-title">Pagination</div>
              <%= partial "/partials/components/elements/pagination" %>

              <div class="section-title">Modal dialogs</div>
              <%= partial "/partials/components/elements/modals" %>
            </div>
          </div>
        </div>

        <div class="row-fluid">
          <div class="span12">
            <div class="padded">
              <div class="section-title">Gauges</div>
              <div class="center">
                <%= partial "/partials/components/elements/gauges" %>
              </div>
            </div>
          </div>
        </div>

      </div>

    </div>
  </div>
</div>