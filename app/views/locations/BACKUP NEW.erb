
<h1> MAPAS </h1>
<div class="contlocation">

  <div id="canvas">  </div>


  <div id="formlocation">

    <%= form_for @location do |f| %>

        <p>
          <%= f.label :address %><br />
          <%= f.text_field :address %>
        </p>
        <p>
          <%= f.label :latitude %><br />
          <%= f.text_field :latitude %>
        </p>
        <p>
          <%= f.label :longitude %><br />
          <%= f.text_field :longitude %>
        </p>

        <p>
          <%= f.label :city %><br />
          <%= f.text_field :city %>
        </p>

        <p><%= f.submit %></p>

    <% end %>


  </div>
</div>




<p><%= link_to "Back to List", locations_path %></p>